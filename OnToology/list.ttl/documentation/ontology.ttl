#################################################################
#    Object Properties
#################################################################

###  http://purl.org/atextor/ontology/list#hasContent
:hasContent rdf:type owl:ObjectProperty ,
                     owl:FunctionalProperty .


###  http://purl.org/atextor/ontology/list#hasNext
:hasNext rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :isFollowedBy ;
         rdf:type owl:FunctionalProperty ,
                  owl:IrreflexiveProperty ;
         rdfs:domain :Element ;
         rdfs:range :Element .


###  http://purl.org/atextor/ontology/list#isFollowedBy
:isFollowedBy rdf:type owl:ObjectProperty ,
                       owl:TransitiveProperty .


#################################################################
#    Classes
#################################################################

###  http://purl.org/atextor/ontology/list#Element
:Element rdf:type owl:Class ;
         rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty :isFollowedBy ;
                           owl:allValuesFrom :Element
                         ] .


###  http://purl.org/atextor/ontology/list#List
:List rdf:type owl:Class ;
      rdfs:subClassOf [ rdf:type owl:Restriction ;
                        owl:onProperty :isFollowedBy ;
                        owl:allValuesFrom :Element
                      ] .


###  http://purl.org/atextor/ontology/list#Terminal
:Terminal rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Class ;
                                owl:oneOf ( :NIL
                                          )
                              ] ;
          rdfs:subClassOf :Element ,
                          [ rdf:type owl:Class ;
                            owl:complementOf [ rdf:type owl:Restriction ;
                                               owl:onProperty :hasNext ;
                                               owl:someValuesFrom owl:Thing
                                             ]
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasContent ;
                            owl:maxCardinality "0"^^xsd:nonNegativeInteger
                          ] .


#################################################################
